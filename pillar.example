varnish:
  listen: ':80'
  storages:
    - 'main=malloc,512m'
    - 'secondary=malloc,256m'
# Parameters to be set on start with the -p param=value option
  params:
    default_ttl: 120
    default_grace: 10
  extra_options: '-w 100,1000,120'
  vcl:
    backend_default_host: 10.1.1.1
    backend_default_port: 80
    files:
      - default.vcl
    files_absent:
      - absent.vcl
